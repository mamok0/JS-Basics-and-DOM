{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-restricted-syntax": "off",
        "no-prototype-builtins": "off",
        "guard-for-in": "off",
        "no-unused-vars": "off",
        "import/extensions": "off",
        "comma-dangle": "off",
        "import/no-cycle": "off"
    }
}